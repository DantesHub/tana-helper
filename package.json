{
  "name": "tana-helper",
  "version": "1.0.0",
  "description": "Localhost http service for Tana commands to call",
  "main": "src/main.ts",
  "bin": "dist/main.cjs",
  "type": "module",
  "scripts": {
    "start": "nodemon ./src/main.ts",
    "build": "rimraf dist && esbuild src/main.ts --bundle --platform=node --outfile=dist/main.cjs",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "dependencies": {
    "@pinecone-database/pinecone": "^0.0.12",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-winston": "^4.2.0",
    "helmet": "^6.1.3",
    "jwks-rsa": "^3.0.1",
    "langchain": "^0.0.73",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "esbuild": "^0.17.18",
    "nodemon": "^2.0.22",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "author": "Brett Adam <verveguy>",
  "license": "MIT"
}
